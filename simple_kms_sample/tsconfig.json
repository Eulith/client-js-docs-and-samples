{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "dist",
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "types": ["node",],
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["src/**/*.spec.ts"]
}
